<h1>Editing teammember</h1>

<% form_for(@teammember) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :eid %><br />
    <%= f.text_field :eid %>
  </p>
  <p>
    <%= f.label :tid %><br />
    <%= f.text_field :tid %>
  </p>
  <p>
    <%= f.label :endda %><br />
    <%= f.date_select :endda %>
  </p>
  <p>
    <%= f.label :begda %><br />
    <%= f.date_select :begda %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @teammember %> |
<%= link_to 'Back', teammembers_path %>
